{
  "private": true,
  "name": "common-ui",
  "scripts": {
    "clean": "lerna clean && rm -rf node_modules",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"packages/legacy-*\" \"packages/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"packages/**/*.js\"",
    "release": "yarn lint && yarn test && lerna publish",
    "report-coverage": "cat coverage/lcov.info | coveralls",
    "test": "jest --coverage"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.19.0",
    "@spraoi/babel-preset": "^0.19.0",
    "@spraoi/helpers": "^0.19.0",
    "apollo-client": "^2.5.1",
    "aws-amplify": "^1.1.26",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.11",
    "buffer": "^4.9.1",
    "core-js": "^2.6.5",
    "final-form": "^4.12.0",
    "gatsby": "^2.3.22",
    "graphql": "^14.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-final-form": "^4.1.0",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-spinners": "^0.5.4",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.19.0",
    "@spraoi/jest-config": "^0.19.0",
    "@spraoi/stylelint-config": "^0.19.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "jest": "^24.7.1",
    "jest-haste-map": "^24.7.1",
    "jest-resolve": "^24.7.1",
    "lerna": "^3.13.2",
    "prettier": "^1.17.0",
    "stylelint": "^10.0.0"
  },
  "resolutions": {
    "@spraoi/base/**/graphql": "^0.11.0",
    "@spraoi/legacy-auth/react-router-dom": "^4.2.2",
    "gatsby/**/graphql": "^0.13.2"
  }
}
