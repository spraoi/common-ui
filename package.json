{
  "private": true,
  "name": "common-ui",
  "scripts": {
    "clean": "lerna clean && rm -rf node_modules",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{js,json,css,scss,md,yml}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"packages/legacy-*\" \"packages/**/*.js\"",
    "lint:styles": "stylelint --ignore-path .gitignore \"packages/**/*.js\"",
    "release": "yarn lint && yarn test && lerna publish",
    "report-coverage": "cat coverage/lcov.info | coveralls",
    "test": "jest --coverage"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@reach/router": "^1.2.1",
    "@spraoi/auth": "^0.20.0",
    "@spraoi/babel-preset": "^0.20.0",
    "@spraoi/helpers": "^0.20.0",
    "apollo-client": "^2.6.0-rc.3",
    "aws-amplify": "^1.1.28",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-styled-components": "^1.10.1",
    "babel-preset-gatsby": "^0.1.11",
    "change-case": "^3.1.0",
    "final-form": "^4.13.0",
    "gatsby": "^2.7.5",
    "graphql": "^14.3.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.0.0",
    "react-final-form-listeners": "^1.0.2",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-spinners": "^0.5.4",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@spraoi/eslint-config": "^0.20.0",
    "@spraoi/jest-config": "^0.20.0",
    "@spraoi/stylelint-config": "^0.20.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-haste-map": "^24.8.0",
    "jest-resolve": "^24.8.0",
    "lerna": "^3.14.1",
    "prettier": "^1.17.1",
    "stylelint": "^10.1.0"
  }
}
